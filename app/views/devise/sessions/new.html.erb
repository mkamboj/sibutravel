<div id="loginFrame">
<a href="#" class="logo"><%= image_tag('sibu.png') %></a>
<div class="loginForm">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
<table>
<tr>
<td><label for="Email">Username</label></td>
<td><%= f.email_field :email, autofocus: true %></td>
</tr>
<tr>
<td><label for="password">Password</label></td>
<td><%= f.password_field :password, autocomplete: "off" %></td>
</tr></table>
<div style="display:none">
        <p class="formLbl">
    <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>
  </p>
  </div>
  <%= hidden_field_tag 'role', 'admin' %>
</dl>
<p class="formLbl">
  <div class="actions">
    <%= f.submit  "Log in" , :class => "btn_blue btnWidth" %>
  </div></p>
<% end %>
</div>

<%= render "devise/shared/links" %>
</div>
